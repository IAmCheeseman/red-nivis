[application]

config/name="Astronaut Game"
run/main_scene="res://UI/MainMenu.tscn"

[autoload]

GameManager="*res://Managing/GameManager.gd"
Memory="*res://Managing/MemoryManager.gd"
Settings="*res://Managing/settings.gd"

[debug]

settings/fps/force_fps=60

[display]

