uniform float darkness;
uniform sampler2D vignette;

void fragment() {
	vec3 vignette_color = texture(vignette, UV).rgb;
	vignette_color += darkness;
	
	COLOR.a -= vignette_color.r;
	COLOR.rgb *= vec3(0, 0, 0);
}
